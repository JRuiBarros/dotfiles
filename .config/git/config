[alias]
	# checkout
	co = checkout
	new = checkout -b
	ren = branch -m
	ma = checkout master
	de = checkout develop
	# commit
	c = commit
	ca = commit -a
	cp = "!git commit; git push"
	cap = "!git commit -a; git push"
	# stash
	sts = stash
	stl = stash list
	stp = stash pop
	stn = stash push -m
	# fetch
	fe = fetch
	pu = pull
	# squash
	ame = "!git add -A; git commit --amend"
	reb = rebase -i
	# reset
	res = reset HEAD~1
	rev = restore .
	# others
	st = status
	di = diff
[pull]
	rebase = true
[credential]
	helper = store
[include]
	path = local
[push]
	autoSetupRemote = true
