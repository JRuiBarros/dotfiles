[alias]
	ame = "!git add -A; git commit --amend"
	back = checkout -
	br = rev-parse --abbrev-ref HEAD
	co = checkout
	new = checkout -b
	mc = "!git merge --no-commit --no-ff $1 && git merge --abort ; "
	pu = pull
	recent = for-each-ref --sort=-committerdate --count=10 --format='%(refname:short)' refs/heads/
	sq = "!git reset --soft HEAD~$1 ; git commit ;"
	st = status
	fi = "!git commit -a -m 'dummy commit'; git rebase -i HEAD~2;"
	fe = fetch
	ma = checkout master
	de = checkout develop
	ct = checkout --track
	cp = "!git commit; git push"
	cap = "!git commit -a; git push"
	re = restore .
	puf = push -f
	stl = stash list
	stp = stash pop
	res = reset HEAD~1
[pull]
	rebase = true
[credential]
	helper = store
[include]
	path = local
